{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "name": "rss-site",
    "labels": {
      "app": "web"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "front-end",
        "image": "nginx",
        "ports": [
          {
            "containerPort": 80,
            "ports": [34,55],
            "status": ["on","off"]
          }
        ]
      },
      {
        "name": "rss-reader",
        "image": "a/rss-php-nginx:v1",
        "ports": [
          {
            "containerPort": 0.23,
            "test": [
              [23,334],
              ["old","new"]
            ]
          }
        ]
      }
    ]
  }
}
